define("static/components/tab/index",function(){var a=function(c,h){var v=this.$nav=$(c);h=this.options=$.extend({},a.DEFAULT,h);var b=this.$contentContainer=$(h.target).length?$(h.target):v.siblings("[data-role=tab-content]");if(0===b.length)throw new Error("can not find tab content");this.$navItems=$(h.navSelector,v),this.$contents=$(h.contentSelector,b),h.mobile&&"ontouchstart"in document&&(h.trigger="click"),this.bindTab()};return a.DEFAULT={target:null,delay:0,navSelector:"> *",contentSelector:"> *",activeClass:"active",trigger:"hover",mobile:!0},a.init=function(){$('[data-toggle="tab"]').each(function(){var a=$(this),c=a.data();a.tab(c)})},a.prototype.bindTab=function(){var a=this,c="hover"===this.options.trigger?"mouseenter":this.options.trigger+".qbt",h=this.$navItems,v=this.$contents;h.on(c,function(c){var b=$(this),g=h.filter("."+a.options.activeClass);if(!$(this).hasClass(a.options.activeClass)){var e=$.Event("show.qbt.tab",{relatedTarget:g});if(b.trigger(e),!e.isDefaultPrevented()){var C=function(){a.clearItemStatus(h),a.clearItemStatus(v),a.activeTab(h.index(b))};a.options.delay&&"hover"===a.options.trigger?(b.stop(!0).delay(a.options.delay).queue(function(a){C(),a()}),b.one("mouseleave.qbt",function(){$(this).stop(!0)})):C(),"click"===a.options.trigger&&c.preventDefault()}}})},a.prototype.activeTab=function(a){var c=this.options.activeClass;this.$navItems.eq(a).addClass(c).trigger({type:"shown.qbt.tab"}),this.$contents.eq(a).addClass(c)},a.prototype.clearItemStatus=function(a){a.removeClass(this.options.activeClass)},$.fn.tab=function(c){return this.each(function(){var h=$(this),v=$(this).data("qbt.tab"),b="object"==typeof c&&c;v||h.data("qbt.tab",v=new a(this,b))})},$(window).on("load",function(){$('[data-toggle="tab"]').each(function(){var a=$(this),c=a.data();a.tab(c)})}),a});